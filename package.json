    "moment-timezone": "^0.5.45",
    "mysql": "npm:mysql2@^3.6.0",
    "nest-aws-sdk": "^3.0.1",
    "nestjs-i18n": "^10.4.5",
    "nestjs-pino": "^4.0.0",
    "nodemailer": "^6.9.10",
    "openai": "^4.28.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pino-pretty": "^10.3.1",
    "redis": "^4.6.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.4",
    "typeorm": "^0.3.20"
